{
  "name": "homie",
  "version": "0.0.1",
  "description": "Homie is a browser AI assistant allows you to chat based on web page content and use your own AI models",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:build": "wxt build --watch",
    "dev:firefox": "wxt -b firefox",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "postinstall": "wxt prepare",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@mozilla/readability": "^0.5.0",
    "@wxt-dev/module-react": "^1.1.1",
    "axios": "^1.7.9",
    "dompurify": "^3.2.2",
    "jsdom": "^25.0.1",
    "marked": "^15.0.4",
    "nanoid": "^5.0.9",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^9.0.1",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0",
    "@types/chrome": "^0.0.281",
    "@types/dompurify": "^3.2.0",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "postcss-rem-to-pixel": "^4.1.2",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2",
    "wxt": "^0.19.15"
  }
}
